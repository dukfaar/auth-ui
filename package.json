    "tsc": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "relay": "relay-compiler --src static/js --schema schema.graphql",
    "relay-watch": "relay-compiler --src static/js --schema schema.graphql --watch",
    "get-schema": "cross-env API_GATEWAY_URL=http://localhost:8090/graphql get-graphql-schema $API_GATEWAY_URL > schema.graphql",
    "build-storybook": "build-storybook"
