{
  "name": "oauth_ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env API_GATEWAY_WS=ws://localhost:8090/socket CLIENT_ID=initialClientId CLIENT_SECRET=initialClientSecret npm run start-dev",
    "start-dev": "webpack-dev-server --progress",
    "build": "webpack",
    "build-docker": "docker build . -t dukfaar/auth-ui",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "relay": "relay-compiler --src static/js --schema schema.graphql",
    "relay-watch": "relay-compiler --src static/js --schema schema.graphql --watch",
    "get-schema": "cross-env API_GATEWAY_URL=http://localhost:8090/graphql get-graphql-schema $API_GATEWAY_URL > schema.graphql",
    "build-storybook": "build-storybook"
  },
  "author": "dukfaar <dukfaar@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "apollo-link": "^1.2.2",
    "apollo-link-ws": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "subscriptions-transport-ws": "^0.9.8",
    "axios": "^0.18.0",
    "css-loader": "^0.28.10",
    "express": "^4.16.3",
    "js-cookie": "^2.2.0",
    "file-loader": "^1.1.9",
    "history": "^4.7.2",
    "@material-ui/core": "^3.0.3",
    "node-cache": "^4.2.0",
    "query-string": "^5.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-transition-group": "^2.3.1",
    "react-relay": "^1.6.0",
    "redux": "latest",
    "redux-thunk": "latest",
    "style-loader": "^0.20.2",
    "typeface-roboto": "^0.0.54",
    "graphql": "^14.0.2"
  },
